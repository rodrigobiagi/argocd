{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "Backoffice": {
    "CreateDatabaseIfNotExists": true,
    "DatabaseServer": "#{DatabaseServer}#",
    "DatabaseName": "BackOffice_Admin",
    "IntegratedSecurity": false,
    "DatabaseUserName": "#{DatabaseUser}#",
    "DatabasePassword": "#{DatabasePass}#",
    "DatabaseCollation": "Latin1_General_CI_AS",
    "IgnoreCertificate": true,
    "KeycloakConfig": {
      "Url": "#{KeycloakConfig.Url}#",
      "Realm": "#{KeycloakConfig.Realm}#",
      "Username": "#{KeycloakConfig.User}#",
      "Password": "#{KeycloakConfig.Pass}#",
      "RoleAdmin": "#{KeycloakConfig.RoleAdmin}#",
      "ClientId": "#{KeycloakConfig.ClientId}#",
      "RedirectUri": "#{KeycloakConfig.RedirectUri}#",
      "ClientIdWeb": "backoffice-web",
      "UsernamePortal": "#{KeycloakConfig.UsernamePortal}#",
      "PasswordPortal": "#{KeycloakConfig.PasswordPortal}#"
    },
    "PortalConfig": {
      "Url": "#{PortalConfig.Url}#"
    },
    "AzureServiceBusConfig": {
      "ConnectionString": "#{AzureServiceBusConfig.ConnectionString}#",
      "Topics": [
        {
          "TopicName": "produto-criado-message",
          "MaxDeliveryCount": 10,
          "CreateTopicIfNotExists": true,
          "MaxSizeInMegabytes": 1024,
          "CreateSubscriptionIfNotExists": false,
          "SubscriptionName": ""
        },
        {
          "TopicName": "grupo-permissao-criado-message",
          "MaxDeliveryCount": 10,
          "CreateTopicIfNotExists": true,
          "MaxSizeInMegabytes": 1024,
          "CreateSubscriptionIfNotExists": false,
          "SubscriptionName": ""
        },
        {
          "TopicName": "permissao-criada-message",
          "MaxDeliveryCount": 10,
          "CreateTopicIfNotExists": true,
          "MaxSizeInMegabytes": 1024,
          "CreateSubscriptionIfNotExists": false,
          "SubscriptionName": ""
        }
      ]
    }
  },
  "KeycloakIdentityServer": {
    "Authority": "#{Keycloak.Authority}#",
    "ClientId": "#{Keycloak.ClientId}#",
    "ClientSecret": "#{Keycloak.ClientSecret}#"
  }
}